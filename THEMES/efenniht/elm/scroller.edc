collections {
   group { name: "elm/scroller/base/default";
      alias: "elm/list/base/default";
      alias: "elm/genlist/base/default";
      alias: "elm/carousel/base/default";
      alias: "elm/gengrid/base/default";

      script {
         public sbvis_v, sbvis_h, sbalways_v, sbalways_h, sbvis_timer;
         public timer0(val) {
            new v;
            v = get_int(sbvis_v);
            if (v) {
               v = get_int(sbalways_v);
               if (!v) {
                  emit("do-hide-vbar", "");
                  set_int(sbvis_v, 0);
               }
            }
            v = get_int(sbvis_h);
            if (v) {
               v = get_int(sbalways_h);
               if (!v) {
                  emit("do-hide-hbar", "");
                  set_int(sbvis_h, 0);
               }
            }
            set_int(sbvis_timer, 0);
            return 0;
         }
      }
      images {
         image: "scrollbar_vertical.png" COMP;
         image: "scrollbar_horizontal.png" COMP;
      }
      parts {
         part { name: "bg";
            type: RECT;
            description { state: "default" 0.0;
               rel1.offset: 1 1;
               rel2.offset: -2 -2;
               color: 255 255 255 0;
            }
         }
         part { name: "clipper";
            type: RECT;
            mouse_events: 0;
            description { state: "default" 0.0;
               rel1.to: "bg";
               rel2.to: "bg";
            }
         }
         part { name: "elm.swallow.content";
            clip_to: "clipper";
            type: SWALLOW;
            description { state: "default" 0.0;
               rel1.offset: 1 1;
               rel2.offset: -2 -2;
            }
         }
         part { name: "sb_vbar_clip_master";
            type: RECT;
            mouse_events: 0;
            description { state: "default" 0.0;
            }
            description { state: "hidden" 0.0;
               visible: 0;
               color: 255 255 255 0;
            }
         }
         part { name: "sb_vbar_clip";
            clip_to: "sb_vbar_clip_master";
            type: RECT;
            mouse_events: 0;
            description { state: "default" 0.0;
            }
            description { state: "hidden" 0.0;
               visible: 0;
               color: 255 255 255 0;
            }
         }
         part { name: "sb_vbar";
            type: RECT;
            mouse_events: 0;
            description { state: "default" 0.0;
               fixed: 1 1;
               visible: 0;
               min: 10 10;
               align: 1.0 0.0;
               rel1 {
                  relative: 1.0 0.0;
                  offset:   -2 3;
               }
               rel2 {
                  relative: 1.0 0.0;
                  offset:   -2 -4;
                  to_y:     "sb_hbar";
               }
            }
         }
         part { name: "sb_vbar_runner";
            clip_to: "sb_vbar_clip";
            type: RECT;
            mouse_events: 0;
            description { state: "default" 0.0;
               min: 1 10;
               max: 1 99999;
               rel1.to:       "sb_vbar";
               rel2.to:       "sb_vbar";
               color_class: "scroller_runner";
            }
         }
         part { name: "elm.dragable.vbar";
            clip_to: "sb_vbar_clip";
            type: RECT;
            mouse_events: 0;
            dragable {
               x: 0 0 0;
               y: 1 1 0;
               confine: "sb_vbar";
            }
            description { state: "default" 0.0;
               fixed: 1 1;
               min: 10 10;
               rel1 {
                  relative: 0.5  0.5;
                  offset:   0    0;
                  to: "sb_vbar";
               }
               rel2 {
                  relative: 0.5  0.5;
                  offset:   0    0;
                  to: "sb_vbar";
               }
               color: 0 0 0 0;
            }
         }
         part { name: "sb_vbar_bar";
            clip_to: "sb_vbar_clip";
            mouse_events: 0;
            description { state: "default" 0.0;
               min: 2 10;
               max: 2 9999;
               image.normal: "scrollbar_vertical.png";
               rel1.to: "elm.dragable.vbar";
               rel2.to: "elm.dragable.vbar";
            }
            description { state: "clicked" 0.0;
               inherit: "default" 0.0;
            }
         }

         part { name: "sb_hbar_clip_master";
            type: RECT;
            mouse_events: 0;
            description { state: "default" 0.0;
            }
            description { state: "hidden" 0.0;
               visible: 0;
               color: 255 255 255 0;
            }
         }
         part { name: "sb_hbar_clip";
            clip_to: "sb_hbar_clip_master";
            type: RECT;
            mouse_events: 0;
            description { state: "default" 0.0;
            }
            description { state: "hidden" 0.0;
               visible: 0;
               color: 255 255 255 0;
            }
         }
         part { name: "sb_hbar";
            type: RECT;
            mouse_events: 0;
            description { state: "default" 0.0;
               fixed: 1 1;
               visible: 0;
               min: 10 10;
               align: 0.0 1.0;
               rel1 {
                  relative: 0.0 1.0;
                  offset:   3 -2;
               }
               rel2 {
                  relative: 0.0 1.0;
                  offset:   -4 -2;
                  to_x:     "sb_vbar";
               }
            }
         }
         part { name: "sb_hbar_runner";
            clip_to: "sb_hbar_clip";
            type: RECT;
            mouse_events: 0;
            description { state: "default" 0.0;
               min: 10 1;
               max: 99999 1;
               rel1.to:       "sb_hbar";
               rel2.to:       "sb_hbar";
               color_class: "scroller_runner";
            }
         }
         part { name: "elm.dragable.hbar";
            clip_to: "sb_hbar_clip";
            type: RECT;
            mouse_events: 0;
            dragable {
               x: 1 1 0;
               y: 0 0 0;
               confine: "sb_hbar";
            }
            description { state: "default" 0.0;
