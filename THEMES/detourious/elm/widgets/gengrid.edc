	alias: "elm/gengrid/item/excessive-image/default";

	data {
		item: "contents" "excessive.swallow.icon";
		item: "labels" "excessive.text";
	}
	parts {
		part {
            type: SWALLOW;
            name: "excessive.swallow.icon";
            mouse_events: 0;
            repeat_events: 0;
            description {
				aspect_preference: BOTH;
				aspect: 1 1;
				rel1.offset: 10 10;
				rel2.offset: -11 -11;
            }
