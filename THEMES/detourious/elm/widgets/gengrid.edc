		part {
            type: RECT;
            name: "excessive.clip";
            mouse_events: 0;
            description {
				rel1 {
					relative: 0.0 1.0;
					offset: 2 -15;
				}
				rel2 {
					offset: -1 0;
				}
				color: 255 255 255 255;
            }
		}
		part {
            type: TEXT;
            effect: SHADOW;
            name: "excessive.text";
            mouse_events: 0;
            scale: 1;
            clip_to: excessive.clip;
            description {
				align: 0.0 0.0;;
				rel1.relative: 0.0 1.0;
				rel1.offset: 2 -15;
				rel2.offset: -1 0;
				TEXT_COLOR;
