	}														\

#define FILEMAN_THUMB_PROGS									\
	program {												\
		name: "thumb_gen";									\
		signal: "e,action,thumb,gen";						\
		source: "e";										\
		action: STATE_SET "visible" 0.0;					\
		target: "icon_box";									\
		/* target: "icon_box_bg"; */						\
		target: "icon_box_shadow";							\
		/* after: "thumb_gen2"; */							\
		after: "thumb_gen6";								\
	}														\
	program {												\
		name: "thumb_gen-";									\
		signal: "e,action,thumb,gen,alpha";					\
		source: "e";										\
		action: STATE_SET "visible" 0.0;					\
		target: "icon_box";									\
		target: "icon_box_bg";								\
		target: "icon_box_shadow";							\
		target: "icon";										\
		/*after: "thumb_gen2";*/							\
	}														\
	program {												\
		name: "thumb_gen2";									\
		action: STATE_SET "min" 0.0;						\
		target: "icon";										\
		after: "thumb_gen3";								\
	}														\
	program {												\
		name: "thumb_gen3";									\
		action: STATE_SET "max" 0.0;						\
		target: "icon";										\
		transition: DECELERATE 0.25;						\
		after: "thumb_gen4";								\
	}														\
	program {												\
		name: "thumb_gen4";									\
		action: STATE_SET "default" 0.0;					\
		target: "icon";										\
		after: "thumb_gen5";								\
	}														\
	program {												\
		name: "thumb_gen5";									\
		action: STATE_SET "max2" 0.0;						\
		target: "icon";										\
		after: "thumb_gen6";								\
	}														\
	program {												\
		name: "thumb_gen6";									\
		action: STATE_SET "default" 0.0;					\
		target: "icon";										\
	}														\


