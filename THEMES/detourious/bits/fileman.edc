group { 
	name: "e/fileman/default/list/fixed";
	alias: "e/fileman/desktop/list/fixed";
	alias: "e/fileman/default/list/variable";
	data.item: "stacking" "below";
	data.item: "selectraise" "on";

	parts {
		part {
			name: "base";
			mouse_events: 0;
			type: RECT;

			description {
				state: "default" 0.0;
				color: COLOR_LIST_BG;
				min: 100 30;
			}

			description {
				state: "selected" 0.0;
