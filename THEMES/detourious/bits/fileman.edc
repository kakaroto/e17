		description {										\
			state: "default" 0.0;							\
			visible: 0;										\
		}													\
		description {										\
			state: "visible" 0.0;							\
			min: 24 24;										\
			max: 24 24;										\
			fixed: 1 1;										\
			visible: 1;										\
			rel1 {											\
				to_x: "icon";								\
				to_y: "icon";								\
