			state: "copy" 0.0;								\
			inherit: "visible" 0.0;							\
			image.normal: "icons/icon_efm_dnd_copy.png";	\
		}													\
		description {										\
			state: "ask" 0.0;								\
			inherit: "visible" 0.0;							\
			image.normal: "icons/icon_efm_dnd_ask.png";		\
		}													\
