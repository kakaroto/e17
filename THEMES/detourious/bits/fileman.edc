			align: 0.5 0.5;									\
			color: 255 255 255 128;							\
			rel1 {											\
				to: "icon";									\
			}												\
			rel2 {											\
				to: "icon";									\
			}												\
		}													\
	}														\
	part {													\
		name: "e.swallow.icon_clip";						\
		type: RECT;											\
		mouse_events: 0;									\
		description {										\
			state: "default" 0.0;							\
