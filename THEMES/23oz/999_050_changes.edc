#define ICON(NAME, FILE, SIZE)                          \
group { name: "e/icons/"NAME ;                          \
   max: SIZE SIZE;                                      \
   parts {                                              \
          part { name: "icon";                              \
         mouse_events:  0;                              \
         description { state: "default" 0.0;            \
                aspect: 1.0 1.0;                            \
                aspect_preference: BOTH;                    \
                image.image: FILE COMP;                     \
                image.normal: FILE ;                        \
         }                                              \
          }                                                 \
   }                                                    \
}


