                visible:  1; \
                rel1.relative:  0.0  0.0; \
                rel1.offset:                 0          0;  \
                rel1.to:        NAME;  \
                rel2.relative:  1.0  0.0; \
                rel2.offset:                 -1          1;  \
                rel2.to:        NAME;  \
