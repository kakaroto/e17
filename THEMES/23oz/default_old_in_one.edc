         name:    "go_active2";
         signal:  "";
         source:  "";
         action:  STATE_SET "hidden" 0.0;
         transition: LINEAR 0.5;
         target:  "background";
         after:   "go_active";
      }
      program {
         name:    "go_passive";
         signal:  "e,state,unfocused";
         source:  "e";
         action:  ACTION_STOP;
         target:  "go_active";
         target:  "go_active2";
         after:   "go_passive2";
      }
      program {
         name:    "go_passiveB";
         signal:  "e,action,hide,label";
         source:  "e";
         action:  STATE_SET "default" 0.0;
         transition: SINUSOIDAL 1.0;
         target:  "e.text.label";
      }
      program {
         name:    "go_passive2";
         signal:  "";
         source:  "";
         action:  STATE_SET "default" 0.0;
         target:  "background";
         script {
             if (get_int(urgent) == 1) {
                 run_program(PROGRAM:"go_urgent");
             }
         }
      }
      program {
         name:    "go_big";
         action:  STATE_SET "visible" 0.0;
         target:  "background";
         after:   "go_big2";
      }
      program {
         name:    "go_big2";
         action:  STATE_SET "huge" 0.0;
         transition: LINEAR 0.5;
         target:  "background";
      }
      program {
         name:    "exec_start";
         signal:  "e,action,start";
         source:  "e";
         action:  ACTION_STOP;
         target:  "go_active";
         target:  "go_active2";
         target:  "go_passive";
         target:  "go_passive2";
         target:  "go_big";
         target:  "go_big2";
         after:   "go_big";
      }
      program {
         name:    "exec_exec";
         signal:  "e,action,exec";
         source:  "e";
         after:   "exec_start";
      }
      program {
         name:    "exec_stop";
         signal:  "e,action,stop";
         source:  "e";
//         action:  SIGNAL_EMIT "active" 0.0;
      }
      program {
         name: "init";
         signal: "load";
         source: "";
         script {
            set_int(urgent, 0);
         }
      }
      program {
         name: "set_urgent";
         signal: "e,state,urgent";
         source: "e";
         script {
            set_int(urgent, 1);
         }
      }
      program {
         name:    "go_urgent";
         signal:  "e,state,urgent";
         source:  "e";
         action:  STATE_SET "visible" 0.0;
         target:  "background";
         after:   "go_urgent2";
      }
      program {
         name:    "go_urgent2";
         action:  STATE_SET "hidden" 0.0;
         transition: LINEAR 0.5;
         target:  "background";
         after:   "go_urgent";
      }
      program {
         name: "unset_urgent";
         signal: "e,state,not_urgent";
         source: "e";
         script {
            set_int(urgent, 0);
         }
      }
      program {
         name:    "go_unurgent";
         signal:  "e,state,not_urgent";
         source:  "e";
         action:  ACTION_STOP;
         target:  "go_urgent";
         target:  "go_urgent2";
         after:   "go_unurgent2";
      }
      program {
         name:    "go_unurgent2";
         action:  STATE_SET "default" 0.0;
         transition: SINUSOIDAL 0.3;
         target:  "background";
      }
   }
}

