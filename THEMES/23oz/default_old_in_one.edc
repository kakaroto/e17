         name:    "set_active";
         signal:  "e,state,selected";
         source:  "e";
         script {
            set_int(active, 1);
         }
      }
      program {
         name:    "set_passive";
         signal:  "e,state,unselected";
         source:  "e";
         script {
            set_int(active, 0);
         }
      }
      program {
         name:   "drag_in";
         signal: "e,action,drag,in";
         source: "e";
         script {
                  if (get_int(drag) == 0) {
               set_state(PART:"overlay", "drag", 0.0);
               set_int(drag, 1);
            }
         }
      }
      program {
         name:   "drag_out";
         signal: "e,action,drag,out";
         source: "e";
         script {
            if (get_int(drag) == 1) {
               if (get_int(active) == 1)
                        set_state(PART:"overlay", "active", 0.0);
               else
                        set_state(PART:"overlay", "default", 0.0);
               set_int(drag, 0);
            }
         }
      }
      program {
         name: "init";
         signal: "load";
         source: "";
         script {
            set_int(active, 0);
            set_int(drag, 0);
         }
      }
   }
