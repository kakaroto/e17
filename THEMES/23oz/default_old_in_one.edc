         }
         description {
            state: "visible" 0.0;
            inherit: "default" 0.0;
            color: 255 255 255 255;
            visible: 1;
         }
      }
   }
   programs {
      program {
         signal: "e,state,checked";
         source: "e";
         action: STATE_SET "visible" 0.0;
         target: "icon";
         transition: LINEAR 0.1;
      }
      program {
         signal: "e,state,unchecked";
         source: "e";
         action: STATE_SET "default" 0.0;
         target: "icon";
         transition: LINEAR 0.1;
      }
   }
}

