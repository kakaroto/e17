group {
   name: "e/widgets/ilist/toggle_end";
   images {
      image: "ilist_active.png" COMP;
   }
   parts {
      part {
         name: "icon";
         type: IMAGE;
         description {
            state: "default" 0.0;
            color: 255 255 255 0;
            visible: 0;
            align: 1.0 0.5;
            aspect: 1.0 1.0;
            aspect_preference: VERTICAL;
            rel1 {
               relative: 0.0  0.0;
               offset: 4    4;
            }
            rel2 {
               relative: 0.0  1.0;
               offset: 4   -5;
            }
            image.normal: "ilist_active.png";
         }
         description {
            state: "visible" 0.0;
            inherit: "default" 0.0;
            color: 255 255 255 255;
            visible: 1;
         }
      }
   }
   programs {
      program {
         name: "set_visible";
         signal: "e,state,checked";
         source: "e";
         action: STATE_SET "visible" 0.0;
         target: "icon";
         transition: LINEAR 0.1;
      }
      program {
         name: "set_invisible";
         signal: "e,state,unchecked";
         source: "e";
         action: STATE_SET "default" 0.0;
         target: "icon";
         transition: LINEAR 0.1;
      }
   }
}
