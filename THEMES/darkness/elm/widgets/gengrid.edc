   group { name: "elm/gengrid/item/thumb/default";
       data {
           item: "icons" "elm.swallow.icon";
           item: "labels" "elm.text";
       }
       images {
           image: "slider_button.png" COMP;
           image: "menu_item_highlight.png" COMP;
           image: "thumb_shadow.png" COMP;
       }
       parts {
           LIST_PART_EVENT
           GENGRID_PART_BG(;)
           part { name: "border-shadow";
               type: IMAGE;
               mouse_events: 0;
               description { state: "default" 0.0;
                   rel1 {
                       to: "elm.swallow.icon";
                       offset: -18 -18;
                   }
                   rel2 {
                       to_x: "elm.swallow.icon";
                       to_y: "elm.text";
                       offset: 17 17;
                   }
                   image {
                       normal: "thumb_shadow.png";
                       border: 17 17 17 17;
                       middle: NONE;
                   }
               }
           }
           part { name: "border";
               type: RECT;
               mouse_events: 0;
               description { state: "default" 0.0;
                   rel1 {
                       to: "border-shadow";
                       offset: 16 16;
                   }
                   rel2 {
                       to: "border-shadow";
                       offset: -15 -15;
                   }
               }
           }
           GENGRID_PART_FG_BASE(,
               rel1.to: "bg";
               rel2.to: "bg";
               rel2.offset: -1 -2;
           )
           part { name: "elm.swallow.icon";
               type: SWALLOW;
               mouse_events: 0;
               description { state: "default" 0.0;
                   aspect_preference: BOTH;
                   aspect: 1.0 1.0;
                   rel1.offset: 0 8;
                   rel2 {
                       to_y: "elm.text";
                       relative: 1.0 0.0;
                       offset: -1 -2;
                   }
               }
           }
           part { name: "elm.text";
               type: TEXT;
               effect: SOFT_SHADOW;
               mouse_events: 0;
               scale: 1;
               description { state: "default" 0.0;
                   color: 255 255 255 255;
                   color3: 0 0 0 0;
                   align: 0.5 1.0;
                   rel1 {
                       relative: 0.0 1.0;
                       offset: 20 -30;
                   }
                   rel2.offset: -21 -15;
                   text {
                       font: "DejaVu";
                       size: 10;
                       min: 0 1;
                       align: 0.5 0.0;
                   }
               }
           }
           FOCUS_GLOW(0.0, 0.0, 0, 0, "bg", 1.0, 1.0, 3, -1, "bg")
       }
       LIST_PROGRAMS(
               target: "bg";
               target: "fg1";
       )
       programs {
          program {
             name: "focus";
             FOCUS_SIGNAL
             FOCUS_GLOW_ACTION("bg")
          }
          program {
             name: "unfocus";
             UNFOCUS_SIGNAL
             UNFOCUS_GLOW_ACTION("bg")
          }
       }
   }
