   group { name: "elm/gengrid/item/default/default";
      data.item: "labels" "elm.text";
      data.item: "icons" "elm.swallow.icon elm.swallow.end";
      images {
         image: "slider_button.png" COMP;
         image: "menu_item_highlight.png" COMP;
      }
      parts {
         LIST_PART_EVENT
         GENLIST_PART_BASE_SHADOW
         LIST_PART_BASE
         LIST_PART_BG(clip_to: "disclip";)
         GENGRID_PART_FG_REL_BG
         GENGRID_PART_SWALLOW_PAD
         GENGRID_PART_SWALLOW_ICON
         part { name: "elm.swallow.end";
            clip_to: "disclip";
            type: SWALLOW;
            description { state: "default" 0.0;
               fixed: 1 0;
               align: 1.0 0.0;
               aspect: 1.0 1.0;
               aspect_preference: HORIZONTAL;
               rel1 {
                  relative: 1.0 0.0;
                  offset: -5 -5;
               }
               rel2 {
                  offset: 5 5;
               }
            }
         }
         part { name: "elm.text";
            clip_to: "disclip";
            type: TEXT;
            effect: SOFT_SHADOW;
            mouse_events: 0;
            scale: 1;
            description { state: "default" 0.0;
               rel1 {
                  relative: 0.0  1.0;
                  offset: 0 0;
               }
               rel2 {
                  relative: 1.0  1.0;
                  offset: -5 -5;
               }
               color: 255 255 255 255;
               color3: 0 0 0 0;
               text {
                  text_class: "Label";
                  font: "DejaVu";
                  size: 10;
                  min: 0 1;
                  align: 0.5 0.0;
               }
            }
            description { state: "selected" 0.0;
               inherit: "default" 0.0;
               color: 224 224 224 255;
               color3: 0 0 0 64;
            }
         }
         FOCUS_GLOW(0.0, 0.0, 0, 0, "bg", 1.0, 1.0, 3, -1, "bg")
         GENLIST_PART_DISCLIP("bg")
      }
      GENLIST_PROGRAMS("bg",
               target:  "bg";
               target:  "fg1";
               target:  "elm.text";
       )
       LIST_PROGRAMS_EVEN_ODD(
            target: "base";
            target: "base_sh";
      )
   }
   group { name: "elm/gengrid/item/default_style/default";
       styles
       {
           style { name: "gengrid_style";
               base: "font=DejaVu font_size=10 align=left valign=0.5 color=#000 text_class=grid_item";
               tag:  "br" "\n";
               tag:  "ps" "ps";
               tag:  "hilight" "+ font=DejaVu:style=Bold";
               tag:  "b" "+ font=DejaVu:style=Bold";
               tag:  "tab" "\t";
           }
           style { name: "gengrid_selected_style";
               base: "font=DejaVu font_size=10 align=left valign=0.5 color=#fff text_class=grid_item";
               tag:  "br" "\n";
               tag:  "ps" "ps";
               tag:  "hilight" "+ font=DejaVu:style=Bold";
               tag:  "b" "+ font=DejaVu:style=Bold";
               tag:  "tab" "\t";
           }
       }
       data.item: "labels" "elm.text";
       data.item: "icons" "elm.swallow.icon elm.swallow.end";
       images {
           image: "slider_button.png" COMP;
           image: "menu_item_highlight.png" COMP;
       }
       parts {
           LIST_PART_EVENT
           GENLIST_PART_BASE_SHADOW
           LIST_PART_BASE
           LIST_PART_BG(clip_to: "disclip";)
           GENGRID_PART_FG_REL_BG
           GENGRID_PART_SWALLOW_PAD
           GENGRID_PART_SWALLOW_ICON
           part { name: "elm.swallow.end";
               clip_to: "disclip";
               type: SWALLOW;
               description { state: "default" 0.0;
                   fixed: 1 0;
                   align: 1.0 0.5;
                   aspect: 1.0 1.0;
                   aspect_preference: VERTICAL;
                   rel1 {
                       relative: 1.0  0.0;
                       offset:   -5    4;
                   }
                   rel2 {
                       relative: 1.0  1.0;
                       offset:   -5   -5;
                   }
               }
           }
           part { name: "elm.text";
               clip_to: "disclip";
               type: TEXTBLOCK;
               mouse_events: 0;
               scale: 1;
               description {
                   state: "default" 0.0;
                   align: 0.0 0.5;
                   fixed: 0 1;
                   rel1 {
                       to_x: "elm.swallow.icon";
                       to_y: "base";
                       relative: 1.0  0.5;
                       offset:   0 4;
                   }
                   rel2 {
                       to_x: "elm.swallow.end";
                       to_y: "base";
                       relative: 0.0  0.5;
                       offset:   -1 -5;
                   }
                   text {
                       style: "Label";
                       min: 1 1;
                   }
               }
               description { state: "selected" 0.0;
                   inherit: "default" 0.0;
                   text {
                       style: "Label";
                   }
               }
           }
           FOCUS_GLOW(0.0, 0.0, 0, 0, "bg", 1.0, 1.0, 3, -1, "bg")
           GENLIST_PART_DISCLIP("bg")
       }
       GENLIST_PROGRAMS("bg",
               target:  "bg";
               target:  "fg1";
               target:  "elm.text";
       )
       programs {
          program {
             name: "select";
             source: "elm";
             signal: "elm,state,selected";
             action: SIGNAL_EMIT "elm,action,focus" "elm";
          }
       }
       LIST_PROGRAMS_EVEN_ODD(
            target: "base";
            target: "base_sh";
      )
   }

