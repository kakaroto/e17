               color: 224 224 224 255;
               color3: 0 0 0 64;
               text {
                  font:     "DejaVu";
                  size:     10;
                  min:      0 0;
                  align:    0.5 0.5;
               }
            }
            description { state: "visible" 0.0;
               inherit: "default" 0.0;
               visible: 1;
               text.min: 1 1;
            }
            description { state: "disabled" 0.0;
               inherit: "default" 0.0;
               color: 0 0 0 128;
               color3: 0 0 0 0;
            }
            description { state: "disabled_visible" 0.0;
               inherit: "default" 0.0;
               color: 0 0 0 128;
               color3: 0 0 0 0;
               visible: 1;
               text.min: 1 1;
            }
         }
         part {          name: "over1";
            mouse_events: 0;
            description { state: "default" 0.0;
