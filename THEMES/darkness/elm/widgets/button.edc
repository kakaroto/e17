            signal: "mouse,up,1";
            source: "button_events";
            action: STATE_SET "default" 0.0;
            target: "button";
         }
         program {
            name: "button_mouseover";
            signal: "mouse,in";
            source: "button_events";
            BUTTON_GLOW_ACTION("button")
         }
         program {
            name: "button_mouseout";
            signal: "mouse,out";
            source: "button_events";
            BUTTON_GLOW_STOP("button")
         }
         program {
            name: "button_focus";
            FOCUS_SIGNAL
            FOCUS_GLOW_ACTION("button")
         }
         program {
            name: "button_unfocus";
            UNFOCUS_SIGNAL
            UNFOCUS_GLOW_ACTION("button")
         }
         program {
            name: "button_drag";
            signal: "repeated";
            source: "elm";
            DRAG_ENTER_ACTION("button")
         }
         BUTTON_GLOW_PROGRAM("button")
         BUTTON_DISABLE_PROGRAM("button")

         program {
            ENABLE_PROGRAM_PART
            target: "disabler";
         }
      }
   }
