program {
   name: "progress_bar_drag_down";
   signal: "mouse,down,1";
   source: "progress_bar_drag";
   action: STATE_SET "down" 0.0;
   target: "progress_bar_drag";
}
program {
   name: "progress_bar_drag_up";
   signal: "mouse,up,1";
   source: "progress_bar_drag";
   action: STATE_SET "play" 0.0;
   target: "progress_bar_drag";
}
