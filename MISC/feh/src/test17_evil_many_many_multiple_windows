#!/bin/bash
[ -x ./feh ] && {
FEH="./feh"
} || {
FEH="feh"
}
echo ""
echo "Hehe. This is an evil test."
echo "This should open all the test images in one window each."
echo "Progressive loading should function, and the process should not"
echo "take too long"
echo ""
$FEH $1 -V -w testimages/*.jpg
echo ""
