	/***************** BUTTON TEXT ELEMENTS *******************************/
                part {
                        name, "previous_text";
			type, TEXT;
			//effect, SOFT_SHADOW;
			mouse_events, 0;		/* Does part accept mouse events? */
//			clip_to, "previous_button";	/* Clip anything outside of the specified object */
