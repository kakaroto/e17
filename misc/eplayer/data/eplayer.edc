                                }
                        }
                        description {
                                state,  "clicked" 0.0;
                                min, 25 20;
                                max, 25 20;

                                rel1 {
                                        relative, 0.0 0.0;
                                        offset, -25 0;
                                        to, "vol_display";
                                }
                                rel2 {
                                        relative, 0.0 1.0;
                                        offset, 0 0;
                                        to, "vol_display";
                                } 

                                image {
