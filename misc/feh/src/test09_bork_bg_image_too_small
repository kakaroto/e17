#!/bin/bash
[ -x ./feh ] && {
FEH="./feh"
} || {
FEH="feh"
}
echo ""
echo "For this test, a background image has been specified which is too"
echo "small to hold all the thumnails. The app should make the user aware"
echo "of this, and recommend a more reasonable size"
echo ""
$FEH $1 -V -o _test09.jpg -m --bg testimages/pngs/bg.png --thumb-width 100 --thumb-height 100 testimages
echo ""
