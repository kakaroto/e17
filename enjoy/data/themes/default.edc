                  offset: 2 2;
               }
               rel2 {
                  to: "bg";
                  offset: -3 -3;
               }
               color: 255 255 255 255;
            }
         }
         part { name: "button";
            type: RECT;
            scale: 1;
            clip_to: "clipper";
            mouse_events: 1;
            dragable {
               x: 1 1 0;
