            scale: 1;
            description { state: "default" 0.0;
               rel1.relative: 1.0 0.0;
               rel1.offset: -4 3;
               rel2.offset: -4 -4;
               align: 1.0 0.5;
               min: 130 30;
               max: 130 30;
               fixed: 1 1;
               image {
                  normal: "toggle_bg.png";
                  border: 3 3 3 3;
               }
            }
         }
         part { name: "clipper";
            type: RECT;
            mouse_events: 0;
            description { state: "default" 0.0;
               rel1 {
