         }
         program { name: "disable_text";
            script {
               new st[31];
               new Float:vl;
               get_state(PART:"elm.text", st, 30, vl);
               if (!strcmp(st, "visible"))
               set_state(PART:"elm.text", "disabled_visible", 0.0);
               else
               set_state(PART:"elm.text", "disabled", 0.0);
            }
         }
         program { name: "enable";
